<UserControl x:Class="NgineUI.App.Views.Network.Optimizer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NgineUI.App.Views.Network"
             xmlns:e="clr-namespace:NgineUI.App.Views.Network.Editors"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Optimizer:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0 3 10 0"/>
            <e:LookupVaueEditor HorizontalAlignment="Left" VerticalAlignment="Top" MinWidth="100"/>
        </StackPanel>
    </Grid>
</UserControl>
